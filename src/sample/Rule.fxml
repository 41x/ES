<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>

<VBox prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.RuleController">
   <children>
      <TextField fx:id="nameTextField" promptText="Name">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </TextField>
      <TextArea fx:id="requestTextField" prefHeight="100.0" prefWidth="200.0" promptText="Reasoning">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </TextArea>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Premises:">
         <VBox.margin>
            <Insets bottom="2.5" left="5.0" top="2.5" />
         </VBox.margin>
      </Text>
      <HBox prefWidth="200.0">
         <children>
            <ComboBox minHeight="32.0" prefWidth="1000.0" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="=">
               <font>
                  <Font size="20.0" />
               </font>
            </Text>
            <ComboBox minHeight="32.0" prefWidth="1000.0">
               <HBox.margin>
                  <Insets right="2.5" />
               </HBox.margin>
            </ComboBox>
            <Button mnemonicParsing="false" text="+">
               <font>
                  <Font size="17.0" />
               </font>
               <HBox.margin>
                  <Insets left="2.5" right="2.5" />
               </HBox.margin>
            </Button>
            <Button minWidth="39.0" mnemonicParsing="false" text="â€“">
               <font>
                  <Font size="17.0" />
               </font>
               <HBox.margin>
                  <Insets left="2.5" />
               </HBox.margin>
            </Button>
         </children>
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="2.5" />
         </VBox.margin>
      </HBox>
      <TableView fx:id="addVarDomainValTableView" prefHeight="150.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </TableView>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Conclusion:">
         <VBox.margin>
            <Insets left="5.0" />
         </VBox.margin>
      </Text>
      <HBox layoutX="15.0" layoutY="179.0" prefWidth="200.0">
         <children>
            <ComboBox minHeight="32.0" prefWidth="1000.0" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="=">
               <font>
                  <Font size="20.0" />
               </font>
            </Text>
            <ComboBox minHeight="32.0" prefWidth="1000.0">
               <HBox.margin>
                  <Insets right="2.5" />
               </HBox.margin>
            </ComboBox>
         </children>
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </HBox>
      <TextArea editable="false" prefHeight="200.0" prefWidth="200.0" promptText="Description">
         <VBox.margin>
            <Insets bottom="15.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </TextArea>
      <HBox alignment="TOP_CENTER" prefWidth="200.0">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
         <children>
            <Button minWidth="100.0" mnemonicParsing="false" onAction="#onOK" prefHeight="32.0" prefWidth="1000.0" text="OK" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets bottom="5.0" right="5.0" />
               </HBox.margin>
            </Button>
            <Button minHeight="32.0" minWidth="100.0" mnemonicParsing="false" onAction="#onCancel" prefWidth="1000.0" text="Close" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
